			var urlPath=window.location.pathname;
	     	var urlPathArray = urlPath.split('/');
	     	if(urlPathArray.indexOf("dashboard") < 0){		
	     		 window.location = contextPath+"/dashboard/home?isViewCourse"; 
	     	}else{
	     		$("#minimize_maximize_myCourse_22").trigger('click',{action:'received'});
	    	}
	 });

	 // to show group received invitations 2x2 view
	 $("#viewGroupNotifications2x2").off("click").bind("click",function(){
	  	closePopups();
	  
		 	var urlPath=window.location.pathname;
	     	var urlPathArray = urlPath.split('/');
	     	//if(urlPathArray[2] != 'dashboard'){
	     	if(urlPathArray.indexOf("dashboard") < 0){		
	     		window.location = contextPath+"/dashboard/home?isViewGroup";
	     	}else{
	     		//$('#minimize_maximize_myGroups_22').attr('fromHeader','true');
	     		//bug fix for XIP-3580 commented all other code and added new trigger event to show only received invitations only.
	   		 	$("#minimize_maximize_receivedInvitation_2x2").trigger('click');
	     		//$('#minimize_maximize_myGroups_22').attr('fromHeader','');
	     		//$("#groupsReceivedInvitationHyperLinkID").trigger('click');
	     		//bug fix for XIP-3580 commented above line and updated with below lines of code.
	     	}
	 });
	 
	 var userPhoto = '1446569045694';
	 updateUserHeaderPhoto(userPhoto);
});
function updateNotificationCount(notificationType){
	var updateNotificationCountOptoins = {
		   url: contextPath+'/feeds/updateAlertCounts?notificationType='+notificationType +'',
		   data:{
			   notificationType:notificationType
		   },
		   successCallBack:function(data){
		   },
		   async:true
	};
	doAjax.ControllerInvocation(updateNotificationCountOptoins);
}
