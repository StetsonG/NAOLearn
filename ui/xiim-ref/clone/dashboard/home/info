<script src="/static/javascript/courses/coursenotification.js"></script>
<script type="text/javascript">
$(document).ready(function(){
	$('#courseNotificationIcon').click(function(){
		 closePopups();
		 
		if(!$(this).closest('li').hasClass('open')){
			$('.header-icons li a .selected-sm').removeClass('selected-sm');
			$(this).coursenotification().closest('li').addClass('open');
			$(this).find('.enabled-sm').addClass('selected-sm');
			// clear notifications count
			updateNotificationCount(3);
			
		}else{
			$(this).find('.enabled-sm').removeClass('selected-sm');
			$(this).closest('li').removeClass('open');
		}
		
	});
	$('body').click(function(e){ 
		if(!($('#courseNotificationIcon').parent().has($(e.target)).length!=0&&$('li.open #courseNotificationIcon').length>0)){
			$('#courseNotificationIcon').find('.enabled-sm').removeClass('selected-sm');
			$('#courseNotificationIcon').closest('li').removeClass('open');
		}
			
	});
});
	
	
</script>
