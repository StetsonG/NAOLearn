	 $("#otherNotifications").off("click").bind("click",function(){
		 
		  closePopups();
		  
		 if($(this).closest('li').hasClass("open")){
		  		$('#otherNotifications').closest('li').find('.group-sm-icons').removeClass('selected-sm');
		  }else{
			  	$('#otherNotifications span').addClass('selected-sm'); 
			  	
				 $('#connectionsDisplay').addClass('hide');
				 $('#otherNotificationsDisplay').removeClass('hide');
				 $("#otherNotifications span").addClass('selected-sm');
				 $("#connectionRequests span").removeClass('selected-sm');
