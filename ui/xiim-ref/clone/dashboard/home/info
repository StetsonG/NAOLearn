			 function shiftingWidgetTwobyTwo(awgd,aw,liid,ele,idd,isOnebyTwo){
					//This line will return if the current widget is already in (2x2) view
					if(!(isOnebyTwo) && ($('#'+idd).attr('data-sizex')==2)){
						return;
					}

					if(isOnebyTwo)
						return shiftingWidgetNRowsbyOneCol(awgd, aw, liid, ele, idd, 2);

					return shiftingWidgetNRowsbyTwoCols(awgd, aw, liid, ele, idd, 2);

				 }
				 

			 function shiftingWidgetOnebyOne(awgd,aw,liid,idd){

						return shiftingWidgetNRowsbyOneCol(awgd, aw, liid, null, idd, 1);
					
			 }			

			//Put all wigets to its default position
			//Assumption: all widgets are already in their default view
  			function resetWidgetPositions(){
                        	var ele = gridster.$el.children();
				var awgd = null;
				var aw = null;
				var rows = [1, -1, 2, 4, 3];
				var nwgd = {
 					col: 0,
					row: 0,
					size_x: 0,
					size_y: 0 
				};

				for(i=0; i< ele.length; i++)
				{
					//skip the smart wall
					if(i == 1)
						i++;

					aw = $(ele[i]);
	 				awgd = aw.coords().grid;
					
					nwgd.col = 1;
					nwgd.row = rows[i];
					nwgd.size_x = awgd.size_x;
					nwgd.size_y = awgd.size_y;

 					gridster.mutate_widget_in_gridmap(aw, awgd, nwgd);
				}	
			}

			//Check all widgets location to avoid using rows beyond 4th row
  			function adjustWidgetPositions(){
                        	var ele = gridster.$el.children();
				var awgd = null;
				var aw = null;
				var row = 1;
				var col = 2;
				var maxcol = gridster.options.max_cols - 1;
				var maxrow = 4;
				var nwgd = {
 					col: 0,
					row: 0,
					size_x: 0,
					size_y: 0 
				};


				for(i=0; i< ele.length; i++)
				{
					aw = $(ele[i]);
	 				awgd = aw.coords().grid;
					
					if(awgd.row > maxrow)
					{
						col = (awgd.col % maxcol) + 1;
						while((row <= maxrow) && gridster.is_widget(col, row))
							row++;
						
						if(row > maxrow)
							break;
						
						nwgd.col = col;
						nwgd.row = row;
						nwgd.size_x = awgd.size_x;
						nwgd.size_y = awgd.size_y;

 						gridster.mutate_widget_in_gridmap(aw, awgd, nwgd);
	
					}
				}	

			}

			function _shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,row_n){
				//Change into N rows by Two columns

				var nwgd = {
 					col: awgd.col,
					row: awgd.row,
					size_x: awgd.size_x,
					size_y: awgd.size_y
				};
				//if the widget is not on the 3rd column (not smartwall)
				// 1. shift to the first cell
 				// 2. expand to two columns
				if(awgd.col + awgd.size_x <= gridster.options.max_cols)
				{
					nwgd.col = 1;
					nwgd.row = 1;
					nwgd.size_x=2;
					nwgd.size_y=row_n;

 					gridster.mutate_widget_in_gridmap(aw, awgd, nwgd);
				}
				else
				{
					nwgd.col = gridster.options.max_cols - 1;
					nwgd.size_x=2;
					nwgd.size_y=row_n;
 					gridster.mutate_widget_in_gridmap(aw, awgd, nwgd);
				}


				$(aw).addClass('widget-width');// css({width:"65%"});
				$(aw).addClass('widget-height');// css({height:"613px"});

 				$('a[liid="'+liid+'"]').attr('minmax','min');
			}

			function _shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,row_n){
				//Change into N rows by One column

				var nwgd = {
 					col: awgd.col,
					row: awgd.row,
					size_x: awgd.size_x,
					size_y: awgd.size_y
				};

				
				if(awgd.col + awgd.size_x <= gridster.options.max_cols)
				{//widgets that are not on the 3rd column

					nwgd.col = 1;
					nwgd.row = 1;
					nwgd.size_x=1;
					nwgd.size_y=row_n;

 					gridster.mutate_widget_in_gridmap(aw, awgd, nwgd);
				}
				else
				{
					nwgd.col = gridster.options.max_cols;
					nwgd.size_x = 1;
					nwgd.size_y = row_n;
 					gridster.mutate_widget_in_gridmap(aw, awgd, nwgd);
				}

				$(aw).addClass('widget-width');// css({width:"65%"});
				$(aw).addClass('widget-height');// css({height:"613px"});

 				$('a[liid="'+liid+'"]').attr('minmax','min');


				return 0;
			}

			function shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,row_n){
				//Shifting with extra processing

				var isRestore = -1;


                                //Only allow smartwall to expand more than 3 rows
                                if((row_n >= 3) && (idd != 2))
                                        return 1;

                                //This line will return if the current widget is already in (nx1) view
                                if(($('#'+idd).attr('data-sizey')== row_n) && ($('#'+idd).attr('data-sizex')==1)){
                                        return 1;
                                }

				if(doRestore)
				{

				 	//isRestore = (idd != 2) && ((awgd.size_x > 1) || (awgd.size_y > 1)) && (row_n == 1);
				 	isRestore = (idd != 2);
					doRestore = false;

                                	//This Line Do The all widgets into default view except the current widget
                                	$('.view-icons.defaultview').not('#'+idd+' .defaultview').trigger('click');

				}

				_shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,row_n);

				//adjust the widgets
				if(isRestore == 1)
				{
					$('#minimize_maximize_Smartwall_22').click();
					//if a widget (other than the smartwall is 2 row by one column, 
					//use 3 rows for the smart wall to keep the maximum row to 4
					if(row_n > 1) 
		    				gridster.resize_widget($('[name="smartwall"]'),2 ,3);

					if(row_n == 1)
						resetWidgetPositions();

				}
		
				if(isRestore != -1)
				{
					adjustWidgetPositions();
					$("html, body").animate({ scrollTop: 0 }, "slow");

					doRestore = true;
				}

				return 0;
			}

			function shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,row_n){
				//Shifting with extra processing
				var isRestore = -1;


                                //Only allow smartwall to expand more than 3 rows
                                if((row_n >= 3) && (idd != 2))
                                        return 1;

                                //This line will return if the current widget is already in (2x2) view
                                if(($('#'+idd).attr('data-sizey')== row_n) && ($('#'+idd).attr('data-sizex')==2)){
                                        return 1;
                                }


				if(doRestore)
				{
					isRestore = 0;
					if(row_n > 3)
						isRestore = 1;

					doRestore = false;

                                	//This Line Do The all widgets into default view except the current widget
                                	$('.view-icons.miniview').not('#'+idd+' .miniview').trigger('click');

				}

				_shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,row_n);

				//This is optional. When smartwall is 4x2, then restore the
				//original positions for all widgets
				if(isRestore == 1)
					resetWidgetPositions();
					
				if(isRestore != -1)
				{
					adjustWidgetPositions();
					$("html, body").animate({ scrollTop: 0 }, "slow");

					doRestore = true;
				}

				return 0;
			}
				 
    </script>
    
