	<script>
	// for displaying success message when account type changed
	if('' == 'true'){
		doAjax.displaySuccessMessage("Account Type changed");
		doAjax.displaySuccessMessagesOnChangeAccountType("false","false");
	}

	
var upgradeAccount = "";
$(document).ready(function(){
var upgradeAccountURI = $("#upgradeAccountURI").val();

upgradeAccount = getModelObject('serviceUrl')+upgradeAccountURI;

$("#saveAccountType").click(function(event){
			 var accountType = $("input[name='accountType']:checked").val();
			 var accountTypeId = $("input[name='accountType']:checked").attr('rel');
			 var accessToken = $("#accessToken_meta").val();
			 var loginUserId = $('#loginUserId-meta').val();
			 var langId = $("#langId_meta").val();
			 /* var changeAccountType = {"accountType":accountType,"accessToken":accessToken, "loginId" : loginUserId};
			 var changeAccountTypeRequest = JSON.stringify(changeAccountType); */ 
			 var headers = {
				accessToken:accessToken,
				langId:langId,
			 };
			 var data = {
			 	accountType:accountType,
			 	loginId:loginUserId
			 };
			  $.ajax({
			       type : 'PUT',
			       url:upgradeAccount+'?accountType='+accountType+'&loginId='+loginUserId,
			       headers:headers,
				   contentType: "application/json",
				   accept: "application/json",
				   cache: false,
				   dataType: "json",
				   //data: JSON.stringify(data),
				   crossDomain: true,
				   async: false,
					error : function(jqXHR, textStatus,errorThrown) {
					},
					success : function(data) {
						if(data['isSuccess'] == 'true'){
							$.ajax({
							url :  contextPath+"/dashboard/changeaccounttype",
							data : 'accountTypeId='+accountTypeId,
							type : 'GET',
							dataType : 'json',
							error : function(jqXHR,textStatus,errorThrown) {
							},
							success : function(data) {
								
								//moveToTop();
								location.reload();
	       						//$("#accountTypeModal").modal('hide');
	       						//doAjax.displaySuccessMessage("Account Type updated successfully");
	       						doAjax.displaySuccessMessagesOnChangeAccountType("true","true");

							}
							});
					    							
						}
						
					}
		 	}); 
		 });
		 
  	});
function moveToTop(){
	$('html, body').animate({scrollTop: 0}, 200);
	return false;
}

//to load user header photo
function updateUserHeaderPhoto(userPhoto){
	 if(userPhoto != '' && userPhoto != null && userPhoto != 'undefined'){
		 $("#userPhotoheaderImage").attr('src','/contextPath/User/'+userPhoto+'/stamp.jpg');
	 }else{
		 $("#userPhotoheaderImage").attr('src',contextPath+'/static/pictures/defaultimages/header-profile_pic-36.png');
	 }
}

// setup notification poll every 30 seconds
function notificationPoll(){
        var interval = "60000";

	if(interval == 'undefined' || interval ==  '')
		interval = 60000;


        setTimeout(function(){
        getAllNotificationsCount();

        //Setup the next poll recursively
        notificationPoll();

        }, interval);
}

function closePopups(){

	  $('.mycourseIcon').popover('destroy');
	  $('.groupShortInfoClassImage').popover('destroy');
	  $('.connectionShortProfileClassImage').popover('destroy');
	  
	  // remove course short profile popups
	  $('.course-shortprofile-popup').html('');
	  $('.course-shortprofile-popup').addClass('hide'); 
}

// getAllNotificationsCount is used to get the count of all notifications
function getAllNotificationsCount(){
	 $.ajax({
		type:'GET',
		url:contextPath+"/feeds/getAlertCounts",
		contentType: "application/json",
        accept: "application/json",
        cache: false,
        dataType: "json",
        async: true,
		error : function(jqXHR, textStatus,errorThrown) {
			
		},
		success : function(data) {
			try{
				data = $.parseJSON(data);
				if(data['friendRequests'] != '' && typeof data['friendRequests'] != 'undefined' && parseInt(data['friendRequests']) > 0){
					$("#connectionRequestCount").removeClass('hide');
				    $("#connectionRequestCount").html(data['friendRequests']);
				}else{
					$("#connectionRequestCount").addClass('hide');
					$("#connectionRequestCount").html('');
				}
				if(data['otherAlerts'] != '' && typeof data['otherAlerts'] != 'undefined' && parseInt(data['otherAlerts']) > 0){
					$("#otherNotificationsCount").removeClass('hide');
				    $("#otherNotificationsCount").html(data['otherAlerts']);
				}else{
					$("#otherNotificationsCount").addClass('hide');
					$("#otherNotificationsCount").html('');
				}
				if(data['privateMessages']!=undefined&&data['privateMessages']>0){
					$('#messageNotificationCount').html(data['privateMessages']).removeClass('hide');
				}else{
					$('#messageNotificationCount').addClass('hide');
				}
				
				// My Notification Count
				if(data['myNotificationsCount']!=undefined&&data['myNotificationsCount']>0){
					$('#my_notification_count').html(data['myNotificationsCount']).removeClass('hide');
				}else{
					$('#my_notification_count').addClass('hide');
				}
				
				// My Notification Count
				if(data['eventAlertsCount']!=undefined&&data['eventAlertsCount']>0){
					$('#calendar_event_count').html(data['eventAlertsCount']).removeClass('hide');
				}else{
					$('#calendar_event_count').addClass('hide');
				}
				// Course Notification Count
				if(data['eventAlertsCount']!=undefined&&data['courseAlertsCount']>0){
					$('#courseRequestCount').html(data['courseAlertsCount']).removeClass('hide');
				}else{
					$('#courseRequestCount').addClass('hide');
				}
			}catch(e){
				//console.log(e);
			}
		}
	}); 
}
</script>
</body>
