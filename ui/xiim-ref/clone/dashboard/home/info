	 contextPath = "" ;
	 serverPort = "80";
	 if(serverPort != '80'){
		 fullContextPath = 'http://xiimapp.xiim.com:80';
	 }else{
		 fullContextPath = 'http://xiimapp.xiim.com';
	 }
	 //fullContextUrl = "http://xiimapp.xiim.com"
	initURLs();
    if($.isFunction(initURLs)){
        //Read in the URLS inserted into the page with JSTL.
        initURLs();
    }
    
    

 	
    
});
function initURLs() {
	// Called first by the code.
	registerModelObject("serviceUrl", "http://xiimapp.xiim.com/XiiMService/Common");
}
function lettersOnly(evt) {
    evt = (evt) ? evt : event;
    var charCode = (evt.charCode) ? evt.charCode : ((evt.keyCode) ? evt.keyCode :
      ((evt.which) ? evt.which : 0));
    if (charCode == 32)
        return true;
    if (charCode > 31 && (charCode < 65 || charCode > 90) &&
      (charCode < 97 || charCode > 122)) {
        return false;
    }
    else
        return true;
}

/**
 * mCustomScrollbar initialization
 * elementid : Element where the custom scroll bar will load (value : "#elementname")
 * isUpdateOnContentResize : isUpdateOnContentResize true or false (value : true/false)
 * setHeight : setHeight for setting the maximum height of the custom scrollbar (Value : "550px")
 * Example : xiimcustomScrollbar("#elementid",true,"550px");
 */
/* function xiimcustomScrollbar(elementid,isUpdateOnContentResize,setHeight){
	 
		if(setHeight == ""){
			setHeight = 0;
		}
	 
	$(elementid).mCustomScrollbar({
		setHeight:setHeight,
		advanced:{updateOnContentResize: isUpdateOnContentResize},
 		mouseWheelPixels: 50,
        mouseWheel:true,
        autoHideScrollbar:false,
        keyboard:{ enable: true},
        contentTouchScroll:25,
        theme: "rounded-dark"
		});
} */
 
/**
 * mCustomScrollbar initialization
 * elementid : Element where the custom scroll bar will load (value : "#elementname")
 * isUpdateOnContentResize : isUpdateOnContentResize true or false (value : true/false)
 * setHeight : setHeight for setting the maximum height of the custom scrollbar (Value : "550px")
 * isDestroy : To destroy the mCustomScrollbar
 */
 function simple_xiimcustomScrollbar(options){
	$(options.elementid).css("overflow-y", "scroll");
	$(options.elementid).css("overflow-x", "hidden");
	$(options.elementid).css("height", options.setHeight);

 }
 function xiimcustomScrollbar(options){
	
	if(options.isDestroy != 'undefined' && options.isDestroy){
		$(options.elementid).mCustomScrollbar("destroy");
		return false;
	}
	
	 var xyaxis = 'y';
	if(options.setHeight != 'undefined' && options.setHeight == ""){
		options.setHeight = 0;
	}
	if(options.horizontal != 'undefined' && options.horizontal == 'x'){
		xyaxis = 'x';
	}
	if(options.vertical != 'undefined' && options.vertical == 'y'){
		xyaxis = 'y';
	}
    if(options.bothSideScroll){
        xyaxis='yx';
    }
	if(options.elementid != 'undefined' && options.setHeight != 'undefined'){
		return $(options.elementid).mCustomScrollbar({
			 setHeight:options.setHeight,
			 advanced:{updateOnContentResize: options.isUpdateOnContentResize,autoScrollOnFocus:true,},
             scrollInertia:0,//Disabled the animation When Dragging/scrolling because it will be annoying to user
		     autoHideScrollbar:false,
		     keyboard:{ enable: true,},
		     /*theme: "rounded-dark",*/
            theme:'dark', // changed the theme because "rounded-dark" theme's Height of scroll thumb is fixed
		    axis:xyaxis,
            callbacks:options.callbacks,
            setTop:options.setTop,
            /* scrollInertia:1000, */
		});
	};
}
 
</script>   
