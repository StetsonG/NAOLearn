<script type="text/javascript">
$(document).ready(function(){
	
	$('#allMessages').on('click',function(){

		 closePopups();
		 
		if(window.location.href.indexOf('/dashboard')!=-1){
		$('#messagesHidden').removeClass('hide');
		myMessages.init({
			isLoadmyMessages:true,
			messageFolder:'INBOX',
			fromHeader:true,
			});
		}else{
			window.location=(contextPath+'/dashboard/home?viewInbox');

			}
		 $('.mynotificationswindowclass').trigger('click');
		});
	
	
	$('#ComposeButton0').on('click',function(){

		if(window.location.href.indexOf('/dashboard')!=-1){
		
		 myMessages.init({
			isComposefromHeader:true,
			fromHeader:true
			});
		}else{
			window.location=(contextPath+'/dashboard/home?composeMesage');
		}
		$('#ComposeButton1').trigger('click');
		$('#messagesHidden').removeClass('hide');
		console.log("mynotificationswindowclass::"+$(".mynotificationswindowclass").hasClass());
		 $('.mynotificationswindowclass').trigger('click');
		});

	$(document).ready(function(){
		if(window.location.href.indexOf('?composeMesage')!=-1){
			$('#messagesHidden').removeClass('hide');
			 myMessages.init({
				isComposefromHeader:true,
				fromHeader:true
				});
			 $('#ComposeButton1').trigger('click');

		}

		if(window.location.href.indexOf('?viewInbox')!=-1){
			
			myMessages.init({
				isLoadmyMessages:true,
				messageFolder:'INBOX',
				fromHeader:true
				});
			$('#messagesHidden').removeClass('hide');

			}

		});
	
	$('#messageNotificationId').on('click', function (event) { 
		
		 closePopups();
		 
		$(this).closest('li').toggleClass("open"); 
		$('#messageNotificationCount').addClass('hide');
		messageNotifications.init({isLoadMessageNotifications:true});
		
		
		$('#connectionRequests span').removeClass('selected-sm');
		 $('#calendar_event_notifications span').removeClass('selected-sm');
		 $('#my_notifications span').removeClass('selected-sm');
		 $('#otherNotifications span').removeClass('selected-sm');
		});
	$('body').on('click', function (e) {
		if(typeof outlineBody=="function"){
			outlineBody(e);
		}
		 if (!$('#messageNotificationId').is(e.target) && $('#messageNotificationId').has(e.target).length == 0 && $('.open').has(e.target).length == 0) {
			 $('#messageNotificationId').closest('li').removeClass('open'); 
	 		}
		 
		 	if (!$('#otherNotifications').is(e.target) && $('#otherNotifications').has(e.target).length == 0 && $('.open').has(e.target).length == 0) {
				$("#otherNotifications span").removeClass('selected-sm');
			}
	  		
			if (!$('#connectionRequests').is(e.target) && $('#connectionRequests').has(e.target).length == 0 && $('.open').has(e.target).length == 0) {
				$("#connectionRequests span").removeClass('selected-sm');
			}
			
			if (!$('#my_notifications').is(e.target) && $('#my_notifications').has(e.target).length == 0 && $('.open').has(e.target).length == 0) {
				$("#my_notifications span").removeClass('selected-sm');
			}
			
			if (!$('#calendar_event_notifications').is(e.target) && $('#calendar_event_notifications').has(e.target).length == 0 && $('.open').has(e.target).length == 0) {
				$("#calendar_event_notifications span").removeClass('selected-sm');
			}
		 $('#messageNotificationId').closest('li').find('.message-sm-icons').removeClass('selected-sm');
		 $('#messageNotificationId').closest('li.open').find('.message-sm-icons').addClass('selected-sm');
		 
	  });
	
	
});
</script>
<script type="text/javascript">
	$("#privacyPolicyModelPopupId").click(function(){
		var options = {
				ele:"#privacy-policy-modal",
				serviceUrl:getModelObject('serviceUrl')+savePasswordURI,
				isPwdTab:true,
				isEmailTab:false
		};
		changepassword.init(options);
		changepassword.colosePopover();
		changepassword.closePopupForPrivacy();
  	  var optionsForModel ={
			  backdrop:'static',
		show:true,
		keyboard:true
	  };
		$("#privacy-policy-modal").modal(optionsForModel);
	});
</script>
