<script type="text-template" id="smartwall-wrapper">
<div class="clear-float pad-bot-10 smartwall-area">
	<div class="popup-processing hide" id="proocessing_smartWall">
			<img src="{{contextPath}}/static/pictures/ajax-loader.gif">
	</div>
	<div class="wall-menus">
		<div>
			<ul>
				<li>
					<div class="dropdown">
						<a href="#" data-toggle="dropdown" class="dropdown-toggle"> <span
							class="up-down-icons enabled-sm"></span>
						</a>
						<ul class="common-dropdown-menus dropdown-menu arrow-right widget-dp">
