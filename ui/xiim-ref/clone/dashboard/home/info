	 $("#connectionRequests").off("click").bind("click",function(){
		
		
		 closePopups();
		  
		if($(this).closest('li').hasClass("open")){
	  		$('#connectionRequests').closest('li').find('.connection-sm-icons').removeClass('selected-sm');
	  	}else{
		  	$('#connectionRequests span').addClass('selected-sm'); 
		  	
			$('#otherNotificationsDisplay').addClass('hide');
			$("#connectionsDisplay").removeClass('hide');
			$("#connectionRequests span").addClass('selected-sm');
			$("#otherNotifications span").removeClass('selected-sm');
			$("#connectionRequests").addClass('ancher_lock');
			$("#connectionRequestsContainer").empty(); 
			
			// Added for My Notifications
			$('#my_notifications_display').addClass('hide');
			$("#my_notifications span").removeClass('selected-sm');
			
			$('#calendar_events_notifications_display').addClass('hide');
			$("#calendar_event_notifications span").removeClass('selected-sm');  
			
			
