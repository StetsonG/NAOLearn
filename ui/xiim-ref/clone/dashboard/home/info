<script type="text/javascript">
        function getRandomInt(min, max) {
            return Math.floor(Math.random() * (max - min + 1)) + min;
        }
</script>
  
<script type="text/javascript">
    /* var jq = jQuery.noConflict();  */ 
    
	var gridster;
	var doRestore = true;

	$(function(){

		gridster = $(".gridster ul").gridster({
			widget_base_dimensions: [294, 294],
			//widget_margins: [12.5, 12.5],
			max_cols: 3,
			helper: 'clone'
		}).data('gridster').disable();
		$('#proocessing_dashboard').remove();
		$('#dashboardgrid').removeClass("hide");
	});
    
    
	$(document).ready(function(){
		 
		 /** added to show hide the options of smart wall**/
		 $("#showOptionsSmartwallID").click(function(e){
			 $("#removeHideSmartwallOptionsID").toggleClass('hide');
		 });
		 
 		$("[id^=minimize_maximize_]").click(function(e){
 			var liid = $(this).attr('liid');
 			var idd = $(this).attr('idd');
 			var ele = gridster.$el.children();
			var aw = $(ele[idd - 1]);
	 		var awgd = aw.coords().grid;
			var nochange = 0;

			$(this).parent().addClass('hide');

	 		restartPopup();
 			/* shifting related to Calendar */
 				if(liid == "Calendar1x2"){
 					nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,2);
					if(nochange)
						return;

 					$('.clickedType').attr('liid',liid);
 					dashboardShifting.toMyCalendar1x2View();
 					//applying dynamic mCustomScrollbar as it is transforming to 2 x 3 and 2 x 4
 					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isDestroy:true};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isUpdateOnContentResize:true,setHeight:"865px",vertical:'y'};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
 	 			}else if(liid == "Calendar2x2"){
 					nochange = shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);
					if(nochange)
						return;
 	 				
 	 				$('.clickedType').attr('liid',liid);
 	 				dashboardShifting.toMyCalendar2x2View();
 	 				
 	 			}else if(liid == "Calendar1x1"){
					//This line will return if the current widget is already in (1x1) view
					if( $('#'+idd).attr('data-sizex')==1 && $('#'+idd).attr('data-sizey')==1){
						$("#removeHideCalendarOptionsID").addClass('hide');
						return;
					} 	 				
 	 				nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid, ele, idd, 1);	
					if(nochange)
						return;

 	 				$('.clickedType').attr('liid',liid);
 	 				dashboardShifting.toMyCalendar1x1View();
 	 				//destroying the mCustomScrollbar if it is being called when 1 x 3 transformation happened
					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isDestroy:true};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
 	 			}
 				
 				
 				/* shifting related to Smart Wall */
 				if(liid == "Smartwall1x2"){
 					nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,4);
					if(nochange)
						return;
			
 					dashboardShifting.toSmartWall1x2View();
 					
 					if($('#baseElementSmartwall').data('xiim-smartwall2')!=undefined){
 	 					$('#baseElementSmartwall').smartwall2('destroy');
 	 				}
 					
 					if($('#baseElementSmartwall').data('xiim-smartwall2x1')!=undefined){
 	 					$('#baseElementSmartwall').smartwall2x1('destroy');
 	 				}
 					
 					$(".dashboard-smartWall-option").removeClass('highlight-menu');
 	 				$(".dashboard-smartWall-option").filter('[data-option="AL"]').addClass('highlight-menu');
 	 				
 	 				$('#baseElementSmartwall').smartwall2x1();
 	 				
 	 				
 					//$("#smartwall-div").addClass("margin-top-minus-13");
 	 			}else if(liid == "Smartwall2x2"){
 	 				nochange = shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,4);
					if(nochange)
						return;

 	 				dashboardShifting.toSmartWall2x2View();
 	 				
 	 				if($('#baseElementSmartwall').data('xiim-smartwall2x1')!=undefined){
 	 					$('#baseElementSmartwall').smartwall2x1('destroy');
 	 				}
 	 				
 	 				if($('#baseElementSmartwall').data('xiim-smartwall2')!=undefined){
 	 					$('#baseElementSmartwall').smartwall2('destroy');
 	 				}
 	 				
 	 				$('#baseElementSmartwall').smartwall2({selectedDashBordOption:$(".dashboard-smartWall-option").filter(".highlight-menu").data('option')});
 	 				
 	 				 /* $('#smartwallMenuToggle').attr('data-toggle','dropdown');
 	 				$("#hideMenuForSmartwall").removeClass('hide');
 	 				$("#removeHideSmartwallOptionsID").addClass('hide'); 
 	 				var mySmartwallFlag={
			  				isSmartwallTwobyTwo:true,
			  				ele:"#baseElementSmartwall"
			  		};
 	 				smartCourses.init(mySmartwallFlag);  */ 	
 	 				// $('#smartWallMainContainer').smartwall();
 	 				
 	 				//$("#smartwall-div").removeClass("margin-top-minus-13");
 	 			}
 				
 				/* shifting related to My Courses */
 				if(liid == "myCourse1x2"){
 	 				nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,2);
					if(nochange)
						return;

 					dashboardShifting.toMyCourses1x2View();
 					
 	 			}else if(liid == "myCourse2x2"){
 	 				nochange = shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);

					if(arguments.length<2) {
						if(nochange)
							return;
						dashboardShifting.toMyCourses2x2View();
					}else {
						dashboardShifting.toMyCourses2x2View(arguments[1].action);
					}
 	 			
 	 			}else if(liid == "myCourse1x1"){
					//This line will return if the current widget is already in (1x1) view
					if( $('#'+idd).attr('data-sizex')==1 && $('#'+idd).attr('data-sizey')==1){
						$("#removeHidemyCourseOptionsID").addClass("hide");
						return;
					} 	 				
 	 				nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid, ele, idd, 1);	
					if(nochange)
						return;

 	 				dashboardShifting.toMyCourses1x1View();
 	 				//destroying the mCustomScrollbar if it is being called when 1 x 3 transformation happened
					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isDestroy:true};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
 	 			}
 				
 				/* shifting related to My Groups */
 				
 				if(liid == "myGroups1x2"){
 					nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,2);
					if(nochange)
						return;

 					$('#baseElementMyGroups').empty();
 					dashboardShifting.toMyGroups1x2View();
 	 			}else if(liid == "myGroups2x2"){
 	 				
 	 				if($('#'+idd+'[data-sizex="1"]').length>0 && $('.groupsULClass').length==0){
 	 					$('#baseElementMyGroups').empty();
 	 					shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);
 	 					dashboardShifting.toMyGroups2x2View();
 	 					return;
 	 				} 	 				

 	 				nochange = shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);
					if(nochange)
						return;

 	 				$('#baseElementMyGroups').empty();

 	 				//checking condition that default (2x2) view should not execute when click event triggered from notifiation toolbar
 	 				if($(this).attr('fromHeader')!='true'){	
 	 				dashboardShifting.toMyGroups2x2View();
 	 				}else{
	 	 				$('#myGroupsMenuToggle').attr('data-toggle','dropdown');
	 	 				$("#hideMenuForMyGroups").removeClass('hide');
	 	 				$("#removeHidemyGroupsOptionsID").addClass('hide');
 	 				}
 	 				
 	 			}
 				//bug fix for XIP-3580 commented written this function to show only recieved invitaitons directly.
 	 			else if(liid == 'groupsReceivedInvitaion2x2'){
	 					$('#baseElementMyGroups').empty();
 	 					shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);
 	 					
						$('#myGroupsFilterIconID').removeClass("disabled-sm");
	 	 				$('#myGroupsMenuToggle').attr('data-toggle','dropdown');
	 	 				$("#hideMenuForMyGroups").removeClass('hide');
	 	 				$("#removeHidemyGroupsOptionsID").addClass('hide');
 	 					var flag={
 	 							isReceivedInvitationsTwobyTwo:true,
 	 							baseElementMyGroups:"#baseElementMyGroups"
 	 					};
 	 					mygroups.init(flag);
 	 			}else if(liid == "myGroups1x1"){
					//This line will return if the current widget is already in (1x1) view
					if( $('#'+idd).attr('data-sizex')==1 && $('#'+idd).attr('data-sizey')==1){
						$("#removeHidemyGroupsOptionsID").addClass("hide");
						return;
					}

 	 				nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid, ele, idd, 1);	
					if(nochange)
						return;

 	 				$('#baseElementMyGroups').empty();
 	 				dashboardShifting.toMyGroups1x1View();
 	 				
 	 				//destroying the mCustomScrollbar if it is being called when 1 x 3 transformation happened
					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isDestroy:true};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
 	 			}
 				
 				
 				/* shifting related to Connections */
 				
 				if(liid == "myConnections1x2"){
 					nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid,ele,idd,2);
					if(nochange)
						return;

 					$('#baseElementMyConnections').empty();
 					dashboardShifting.toMyConnections1x2View();
 					//applying dynamic mCustomScrollbar as it is transforming to 2 x 3 and 2 x 4
					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isDestroy:true};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isUpdateOnContentResize:true,setHeight:"865px",vertical:'y'};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
 	 			}else if(liid == "myConnections2x2"){
 	 				
 	 				if($('#'+idd+'[data-sizex="1"]').length>0 && $('.myConnectionsDivULClass').length==0){
 	 					$('#baseElementMyConnections').empty();
 	 					shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);
 	 					if($(this).attr('fromHeader')!='true'){
 	 						dashboardShifting.toMyConnections2x2View();
 	 					}
 	 					return;
 	 				} 	 				

 	 				nochange = shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);
					if(nochange)
						return;

 	 				$('#baseElementMyConnections').empty();
 	 				//checking condition that default (2x2) view should not execute when click event triggered from notifiation toolbar
 	 				if($(this).attr('fromHeader')!='true'){
 	 				dashboardShifting.toMyConnections2x2View();
 	 				}else{
	 	 				$('#myconnectionsMenutoggle').attr('data-toggle','dropdown');
	 	 				$("#hideMenuForMyConnections").removeClass('hide');
	 	 				$("#removeHidemyConnectionsOptionsID").addClass('hide');
 	 				}
 	 				
 	 				
 	 			}
 				
 	 			else if(liid == 'myConnectionsReceivedInvitaion2x2'){
 	 				
 	 				
	 					$('#baseElementMyConnections').empty();
 	 					shiftingWidgetNRowsbyTwoCols(awgd,aw,liid,ele,idd,2);
 	 					
 	 					
	 	 				$('#myconnectionsMenutoggle').attr('data-toggle','dropdown');
	 	 				$("#hideMenuForMyConnections").removeClass('hide');
	 	 				$("#removeHidemyConnectionsOptionsID").addClass('hide');
						$('#myconnectionsfiltericonid').removeClass("disabled-sm");
 	        		  
 	        		  if($("#connectionRequestCount:visible").length > 0){
 		        		  // Used update the connections notification count 
 		        		  updateNotificationCount(4);
 		        		  $("#connectionRequestCount").addClass('hide');
 	        		  }
 	        		  var flag;
 	        		  if($('#5').attr('data-sizex')=='1')
 							flag={
 	    		  				isMyConnectionsPendingRequest:true,
 	    		  				baseElementMyConnections:"#baseElementMyConnections"
 	    		  		};	
 	        		  else{
 	        			  flag={
 	      		  				isMyConnectionsPendingRequestTwobyTwo:true,
 	      		  			baseElementMyConnections:"#baseElementMyConnections"
 	      		  		};	
 	        		  }
 	    				myconnections.init(flag); 
 	            	   	 				
 	 				
 	 			}else if(liid == "myConnections1x1"){
					//This line will return if the current widget is already in (1x1) view
					if( $('#'+idd).attr('data-sizex')==1 && $('#'+idd).attr('data-sizey')==1){
						$('#removeHidemyConnectionsOptionsID').addClass('hide');
						return;
					}

 	 				nochange = shiftingWidgetNRowsbyOneCol(awgd,aw,liid, ele, idd, 1);	
					if(nochange)
						return;

 	 				$('#baseElementMyConnections').empty();
 	 				dashboardShifting.toMyConnections1x1View();
 	 				//destroying the mCustomScrollbar if it is being called when 1 x 3 transformation happened
					var xiimcustomScrollbarOptions = {elementid:"#smartwall-div",isDestroy:true};
					xiimcustomScrollbar(xiimcustomScrollbarOptions);
 	 			}

		 });
	 });

	function restartPopup(){
 		if($('.popover').hasClass('in')){
 			$(".popover.in").each(function(){
 	 			var open_popup_id = $(this).attr('id');
 	 			var popup_open_ele = $('span[aria-describedby="'+open_popup_id+'"],div[aria-describedby="'+open_popup_id+'"],li[aria-describedby="'+open_popup_id+'"],td[aria-describedby="'+open_popup_id+'"],i[aria-describedby="'+open_popup_id+'"]');
 		 		if(popup_open_ele){
 		 			popup_open_ele.popover('destroy');
 		 		}
 			});
 		}
 			
 		// Code is added to remove the active class in smart wall 2x2 new post icon
 		$("#newPostID").removeClass('selected-sm');
 	}

    	//Added For The Case Insesensitive Search Functionality 
    	$.extend($.expr[":"], {
    		"containsIN": function(elem, i, match, array) {
    		return (elem.textContent || elem.innerText || "").toLowerCase().indexOf((match[3] || "").toLowerCase()) >= 0;
    		}
    	});
		 
  
	$(document).ready(function(){
			 
			//by default smart wall should be in 1x2 view
		    	//gridster.resize_widget($("#2"),2 ,4);

			//1. change the smartwall into the default position
			$('#minimize_maximize_Smartwall_22').click();

			//2. set up "My Calendars"
			initMyCalendar();

			//3. set up "My Courses"
			initMyCourses();

			//4. set up "My Groups"
			initMyGroups();

			//5. set up "My Connections"
			initMyConnections();

			    $('body').on('click',function(e){
			    	if($(e.target).parents('.ui-autocomplete').length === 0 &&  $(e.target).parents('.popover.in').length === 0 && $(e.target).parents('.popoverParents').length === 0 && $(e.target).parents('.closePopover').length === 0){
			    		
			    		restartPopup();
			    	}
			    });
			    
   			    $(".gridster").click(function(e){
 			    	
 			    	$('.over_popover').each(function () {
 			            //the 'is' for buttons that trigger popups
 			            //the 'has' for icons within a button that triggers a popup
 			                if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && 				$('.popover').has(e.target).length === 0) {
 			                $(this).popover('hide');
 			            }
 			        });
 			    	if($('#my_notifications_2X2_view').is(':visible')) {
			    		myNotifications.close2x2MyNotifications(this);
 			    	}
			    	
			    });   
			 
			$("#viewProfileClose").click(function(e){
				$("#viewConnetionProfile").addClass('hide');
			});
			
			var isViewUserProfile = "false";
